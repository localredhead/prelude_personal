  (add-hook 'vc-dir-mode-hook 'turn-on-diff-hl-mode)
  
  ;; Makes scrolling smoother.
- (setq redisplay-dont-pause t
-       scroll-margin 1
-       scroll-step 1
-       scroll-conservatively 10000
-       scroll-preserve-screen-position 1)
  
  ;;Navigate between emacs window panes.
